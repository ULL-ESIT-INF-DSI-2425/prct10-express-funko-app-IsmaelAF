{
  "name": "doc-test",
  "version": "1.0.0",
  "main": "basicFunctions.js",
  "type": "module",
  "scripts": {
    "start": "tsc-watch",
    "dev": "tsc-watch --onSuccess \"node dist/basicFunctions.js\"",
    "doc": "typedoc --out ./docs ./src/**/*.ts",
    "test": "vitest run",
    "coverage": "vitest run --coverage --coverage.include src/ejercicio-{1,2}/* --coverage.exclude src/ejercicio-{1,2}/app.ts --coverage.exclude src/ejercicio-{1,2}/server.ts --coverage.exclude src/ejercicio-{1,2}/server2.ts --coverage.exclude src/ejercicio-{1,2}/cliente.ts --coverage.exclude src/ejercicio-{1,2}/cliente2.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@types/node": "^22.13.10",
    "@types/yargs": "^17.0.33",
    "@vitest/coverage-v8": "^3.0.5",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-tsdoc": "^0.4.0",
    "globals": "^15.15.0",
    "prettier": "^3.5.1",
    "tsc-watch": "^6.2.1",
    "typedoc": "^0.27.7",
    "typescript-eslint": "^8.24.1",
    "vitest": "^3.0.5"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "yargs": "^17.7.2"
  }
}
